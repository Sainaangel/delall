{
  "name": "Pyrogram Delete All Bot",
  "description": "Telegram bot to delete messages from telegram channel.",
  "logo": "https://telegra.ph/file/7ec22c82f580a334dd13e.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/CrazeBots/deleteAllbot",
  "env": {
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Obtain a Telegram bot token by contacting @BotFather",
      "required": true,
      "value": ""
    },
    "OWNERID": {
      "description": "Your Telegram id by contacting @MissRose_bot",
      "required": false,
      "value": ""
    }
    "UserBotID": {
      "description": "Your User Account Telegram id by contacting @MissRose_bot",
      "required": true,
      "value": ""
    }
    "SESSION_STRING": {
      "description": "pyrogram session string",
      "required": true,
      "value": ""
    }
    "LOG_CHANNEL": {
      "description": "Log channel for bot, must start with -100**********",
      "required": true,
      "value": ""
    }
    
  }
}